<!DOCTYPE html>
<html>
<head>
  <title>RH</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<header>
  <div id="header-wrap">
    <h2 align="center"> RH - DADOS GERAIS </h2>
  </div>
</header>

<nav class="navbar navbar-inverse" role="navigation">
  <div class="container">

    <ul class="nav navbar-nav">

      <li class="lead"> <%= link_to 'Setores', setors_path %></li>


      <li class="dropdown">
        <a href="#" class="dropdown-toggle lead" data-toggle="dropdown"> Pessoas &nbsp; <b class="caret"></b></a>
        <ul class="dropdown-menu" role="menu">
          <li role="presentation" class="lead"> <%= link_to "Todos", pessoas_path %></li>
          <li role="presentation" class="lead"> <%= link_to "Critério de busca", escolhe_relatorio_path %></li>
          <li role="presentation" class="lead"> <%= link_to 'Chefias', chefias_path %></li>
        </ul>
      </li>

      <li class="dropdown">
        <a href="#" class="dropdown-toggle lead" data-toggle="dropdown"> Títulos &nbsp; <b class="caret"></b></a>
        <ul class="dropdown-menu" role="menu">
          <li role="presentation" class="lead"> <%= link_to "Docentes", titulos_path %></li>
          <li role="presentation" class="lead"> <%= link_to "Funcionários", titulosfunc_path %></li>
        </ul>
      </li>

      <li class="lead"><%= link_to 'Experiência profissional', profissional_path %></li>

      <li class="lead"><%= link_to 'Histórico de dados', historico_path %></li>

      <li class="lead"><%= link_to 'Participações em colegiados', colegiados_path %></li>

      <li class="lead"><%= link_to 'Suplências', suplencias_path %></li>

      <!--li class="lead">< %= link_to 'Validação de títulos', validacao_path %></li-->

    </ul>
  </div>
</nav>
<br>

<%= yield %>

</body>
</html>
